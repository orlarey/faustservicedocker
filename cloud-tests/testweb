#!/bin/bash

# Enviroment variable that can be set a default faustservice url
[ ! -z "$1" ] && SERVICE="$1" || SERVICE=${CLOUDSERVICE:="https://faustservicecloud.grame.fr"}

PLATFORM=web

./rtest $SERVICE $PLATFORM wam2-poly-ts kisana.dsp 88100
./rtest $SERVICE $PLATFORM wam2-ts kisana.dsp 88100
./rtest $SERVICE $PLATFORM wasmjs-poly-ts kisana.dsp 88100
./rtest $SERVICE $PLATFORM wasmjs-ts kisana.dsp 88100
./rtest $SERVICE $PLATFORM webaudiowasm-ts kisana.dsp 88100
./rtest $SERVICE $PLATFORM wap kisana.dsp 8690
./rtest $SERVICE $PLATFORM wap-poly kisana.dsp 8700
./rtest $SERVICE $PLATFORM wasmjs kisana.dsp 13600
./rtest $SERVICE $PLATFORM wasmjs-poly kisana.dsp 16500
./rtest $SERVICE $PLATFORM wasmjs-worklet kisana.dsp 26400
./rtest $SERVICE $PLATFORM webaudiowasm kisana.dsp 97000
./rtest $SERVICE $PLATFORM webaudiowasm-links kisana.dsp 563900
./rtest $SERVICE $PLATFORM webaudiowasm-poly kisana.dsp 88100
./rtest $SERVICE $PLATFORM webaudiowasm-worklet kisana.dsp 98800
